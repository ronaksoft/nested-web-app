worker_processes  1;

events {
    worker_connections  1024;
}


http {
    default_type  application/octet-stream;

    sendfile        on;
    keepalive_timeout  65;

    gzip  on;

    server {
        listen       80;
        server_name  localhost;

        location / {
            root   /ronak/nested/webapp;
            index  index.html;
        }

        location /getConfig/ {
            proxy_pass https://npc.nested.me/dns/discover/;
        }
    }
}
